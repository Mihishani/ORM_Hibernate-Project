<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="Anchorpane1" prefHeight="900.0" prefWidth="1485.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.orm.gdse.Controller.UserLoginController">
   <children>
      <ImageView fx:id="pane2" fitHeight="900.0" fitWidth="1485.0" opacity="0.86">
         <image>
            <Image url="@../assets/MainPage.gif" />
         </image>
      </ImageView>
      <TextField fx:id="txtUserName" layoutX="497.0" layoutY="359.0" onAction="#txtUserNameOnaction" prefHeight="56.0" prefWidth="596.0" promptText="User_Name..." style="-fx-background-color: #a67b5b;">
         <font>
            <Font name="System Italic" size="18.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txtPassword" layoutX="497.0" layoutY="469.0" onAction="#txtpasswordonAction" prefHeight="56.0" prefWidth="596.0" promptText="password..." style="-fx-background-color: #a67b5b;">
         <font>
            <Font name="System Italic" size="18.0" />
         </font>
      </PasswordField>
      <Button fx:id="btnLogin" layoutX="694.0" layoutY="624.0" mnemonicParsing="false" onAction="#btnLoginonAction" prefHeight="70.0" prefWidth="173.0" style="-fx-background-color: gold;" text="LOGIN">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Button>
      <ImageView fitHeight="161.0" fitWidth="162.0" layoutX="699.0" layoutY="125.0">
         <image>
            <Image url="@../assets/black-male-user-symbol.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
